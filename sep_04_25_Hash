#include<stdio.h>

int main(){
    int arr[] = {3,5,6,1,2};
    int target=6;
    int size = sizeof(arr)/sizeof(arr[0]);
    int max=arr[0];
    for (int i=1;i<size;i++){
        if(arr[i]>max){
            max=arr[i];
        }
    }
    int hash[max+1];
    for(int i= 0; i<max+1;i++){
        hash[i]=0;
    }
    for(int i=0; i<size; i++){
        hash[arr[i]]++;

    }
    if(hash[target]==0){
        printf("Target is not found");
    }else{
        printf("Target is found");
    }
    return 0;

}
